{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@common/*": ["../common/*"],  
      "@client/*": ["../client/*"],
      "@assets/*": ["../client/assets/*"] 
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    ".expo/types/**/*.ts"
  ]
}
